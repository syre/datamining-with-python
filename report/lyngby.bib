  title = 	 {Ridge Regression: Biased estimation for
                  nonorthogonal problems},
  journal = 	 {Technometrics},
  year = 	 {1970},
  OPTkey = 	 {},
  volume = 	 {12},
  number = 	 {1},
  month = 	 {February},
  pages = 	 {55--67},
  OPTnote = 	 {},
  OPTannote = 	 {},
  obtain =       {copy},
  affiliation =  {University of Delaware; E. I. du Pont de Nemours \&
                  Co.},
  keywords =      {ridge regression},
  abstract =     {In multiple regression it is shown that parameter
                  estimates based on minimum residual sum of squares
                  have high probability of being unsatisfactory, if
                  not incorrect, if the prediction vectors are not
                  orthogonal. Proposed is an estimation procedure
                  based on adding small positive quantities to the
                  diagonal of ${\bf X}'{\bf X}$. Introduced is the
                  ridge trace, a method for showing in two dimensions
                  the effects of nonorthogonality. It is then shown
                  how to augment ${\bf X}'{\bf X}$ to obtain biased
                  estimates with smaller mean square error.},  
}

