  author = 	 {Eugene Charniak},
  title = 	 {Statistical Techniques for Natural Language
                  Parsing},
  journal = 	 {AI Magazine},
  year = 	 {1997},
  OPTkey = 	 {},
  OPTvolume = 	 {},
  OPTnumber = 	 {},
  OPTmonth = 	 {},
  OPTpages = 	 {},
  OPTnote = 	 {},
  OPTannote = 	 {},
  affiliation =  {Department of Computer Science, Brown University},
  abstract =     {We review current statistical work on syntactic
                  parsing and then consider part-of-speech tagging,
                  which was the first syntactic problem to be
                  successfully attacked by statistical techniques and
                  also serves as a good warmup for the main topic,
                  statistical parsing. Here we consider both the
                  simplified case in which the input string is viewed
                  as a string of parts of speech, and the more
                  interesting case in which the parser is guided by
                  statistical information about the particular words
                  in the sentence. Finally we anticipate future
                  research directions.},
}

