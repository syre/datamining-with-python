@Article{Tikochinsky1984Alternative,
  author = 	 {Y. Tikochinsky and N. Z. Tishby and R. D. Levine},
  title = 	 {Alternative Approach to maximum-entropy inference},
  journal = 	 {Physical Review A},
  year = 	 {1984},
  OPTkey = 	 {},
  volume = 	 {30},
  number = 	 {5},
  month = 	 {November},
  pages = 	 {2638--2644},
  OPTnote = 	 {},
  OPTannote = 	 {},
  affiliation =  {The Fritz Haber Molecular Dynamics Reasearch Center,
                  The Hebrew University of Jerusalem, Jerumsalem,
                  Israel},
  abstract =     {A consistent approach to the inference of a
                  probability distribution given a limited number of
                  expectation values of relevant variables is
                  discussed. There are two key assumptions: that the
                  experiment can be independently repeated a finite
                  number (not necessarily large) of times and that the
                  theoretical expection values of the relevant
                  observables are to be estimated from their measured
                  sample averages. Three independent but complementary
                  routes for deriving the form of the distribution
                  from these two assumptions are reviewed. All three
                  lead to a unique distribution which is identical
                  with the one obtained by the maximum-entropy
                  formalism. The present derivation thus provides an
                  altenative approach to the inference problem which
                  does not invoke Shannon's notion of missing
                  information or entropy. The approach is more limited
                  in scope than the one proposed by Jaynes, but has
                  the advantage that it is objective and that the
                  operational origin of the ``given'' expection values
                  is specified.},
}

