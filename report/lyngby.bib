@MastersThesis{Sigurdur1998AnalysisPhysiologicalConfounds,
  author = 	 {Sigurdur Sigurdsson},
  title = 	 {Analaysis of Physiologival Confounds in Funcional
                  Magnetic Resonance Signals},
  school = 	 {Department of Mathematical Modelling, Technical
                  University of Denmark},
  year = 	 {1998},
