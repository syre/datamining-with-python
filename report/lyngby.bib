@Article{Brooks1998Markov,
  author = 	 {Stephen P. Brooks},
  title = 	 {Markov chain {Monte Carlo} method and its application},
  journal = 	 {the Statistician},
  year = 	 {1998},
  OPTkey = 	 {},
  volume = 	 {47},
  OPTnumber = 	 {},
  OPTmonth = 	 {},
  pages = 	 {69--100},
  OPTnote = 	 {},
  OPTannote = 	 {},
  affiliation =  {University of Bristol, UK},
  keywords =     {Bayesian statistics; Gibbs sampler;
                  Metropolis-Hastings updating; simulation; software},
  abstract =     {The Markov chain Monte Carlo (MCMC) method, as a
                  computer-intensive statistical tool, has enjoyed
                  enormous upsurge in interest over the last few
                  years. This paper provides a simple, comprehensive
                  and tutorial review of some of the most common areas
                  of research in this field. We begin by discussing
                  how MCMC algorithms can be constructed from standard
                  building-clocks to produce Markov chains with the
                  desired stationary distribution. We also motivate
                  and discuss more complex ideas that have been
                  proposed in the literature, such as continuous time
                  and dimension jumping methods. We discuss some
                  implementational issues associated with MCMC
                  methods. We take a look at the arguments for and
                  against multiple replications, consider how long
                  chains should be run for and how to determine
                  suitable starting points. We also take a look at
                  graphical models and how graphical approaches can be
                  used to simplify MCMC implementation. Finally, we
                  present a couple of examples, which we use as
                  case-studies to highlight some of the points made
                  earlier in the text. In particular, we use a simple
                  changepoint model to illustate how to tackle a
                  typical Bayesian modelling problem via the MCMC
                  method, before using mixture model probles to
                  provide illustrations of good sampler output and of
                  the implementation of a reversible jump MCMC algorithm.},
}

