@Article{Hansen1996Linear,
  author = 	 {Lars Kai Hansen and Jan Larsen},
  title = 	 {Linear Unlearning for Cross-Validation},
  journal = 	 {Advances in Computational Mathematics},
  year = 	 {1996},
  OPTkey = 	 {},
  volume = 	 {5},
  OPTnumber = 	 {},
  OPTmonth = 	 {},
  pages = 	 {269--280},
  OPTnote = 	 {},
  OPTannote = 	 {},
  url =          {http://isp.imm.dtu.dk},
  keywords =     {cross-validation; neural network},
  abstract =     {The leasve-one-out cross-validation scheme for
                  generalization assessment of neural network models
                  is computationally expensive due to replicated
                  training sessions. In this paper we suggest linear
                  unlearning of examples as an approach to
                  approximative croos-validation. Further, we discuss
                  the possibility of exploiting the ensemble of
                  networks offered by leave-one-out for performing
                  ensemble predictions and for obtaining error bars on
                  future examples. We show that 1) the optimal
                  ensemble prediction, based on linear combinations of
                  the individual networks, is obtained by weighting
                  the networks equally, and 2) that the generalization
                  performance of the ensemble predictor is identical
                  to that of the network trained on the whole training
                  set.

                  Numerical experiments on the sunspot time series
                  prediction benchmark demonstrates the potential of
                  the linear unlearning technique.},
}

