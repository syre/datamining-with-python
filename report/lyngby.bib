@Article{Woods1993MRIPET,
  author = 	 {Roger P. Woods and John C. Mazziotta and S. R. Cherry}, 
  title = 	 {{MRI-PET} registration with automated algorithm},
  journal = 	 {Journal of Computer Assisted Tomography},
  year = 	 {1993},
  OPTkey = 	 {},
  volume = 	 {17},
  number = 	 {4},
  OPTmonth = 	 {},
  pages = 	 {536--546},
  OPTnote = 	 {},
  OPTannote = 	 {},
  keywords =     {MRI; brain; compuerised tomography; medical image
                  processing; radioisotope scanning and imaging},
  abstract =     {The authors have previously reported an automated
                  method for within-modality (e.g., PET-to-PET) image
                  alignment. In the present work, they describe
                  modifications to this method that allow for
                  cross-modality registration of MRI and PET brain
                  images obtained from a single subject. To align the
                  images, the algorithm seeks to minimize the standard
                  deviation of the PET pixel values that correspond to
                  each MRI pixel value. The MR images must be edited
                  to exclude nonbrain regions prior to using the
                  algorithm. The method has been validated
                  quantitatively using data from patients with
                  stereotaxic fiducial markers rigidly fixed in the
                  skull. Maximal three-dimensional errors of <3 mm and
                  mean three-dimensional errors of <2 mm were
                  measured. Computation time on a SPARCstation IPX
                  varies from 3 to 9 min to align MR image sets with
                  (/sup 18/F)fluorodeoxyglucose PET images. The MR
                  alignment with noisy H/sub 2//sup 15/O PET images
                  typically requires 20-30 min}, 
}

