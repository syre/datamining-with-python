@InProceedings{Kay1992Feature,
  author = 	 {Jim Kay},
  title = 	 {Feature Discovery under Contextual Supervision using
                  Mutual Information},
  booktitle = 	 {Proceedings of the 1992 International Joint
                  Conference on Neural Networks},
  OPTcrossref =  {},
  OPTkey = 	 {},
  OPTeditor = 	 {},
