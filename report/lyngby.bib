@InProceedings{Larsen1994Generalization,
  author = 	 {Jan Larsen and Lars Kai Hansen},
  title = 	 {Generalization Performance of Regularized Neural
                  Network Models},
  OPTbooktitle = {},
  crossref =     {Vlontzos1994NNSP},
  OPTkey = 	 {},
  OPTeditor = 	 {},
  OPTvolume = 	 {},
  OPTnumber = 	 {},
  OPTseries = 	 {},
  OPTyear = 	 {},
  OPTorganization = {},
  OPTpublisher = {},
  OPTaddress = 	 {},
  OPTmonth = 	 {},
  pages = 	 {42-51},
  OPTnote = 	 {},
  OPTannote = 	 {},
  affiliation =  {Electronics Institute, Technical University of Denmark},
  url =          {http://isp.imm.dtu.dk},
  keywords =      {artificial neural networks, generalization,
                  regularization, FPE, GPE, FPER},
  abstract =     {Architecture optimization is a fundamental problem
                  of neural network modeling. The optimal architecture
                  is defined as the one which minimizes the
                  generalization error. This paper addresses
                  estimation of the generalization performance of
                  regularized, complete neural network
                  models. Regularization normally improves the
                  generalization performance by restricting the model
                  complexity. A formula for the optimal weight decay
                  regularizer is derived. A regularized model may be
                  characterized by an effective number of weights
                  (parameters); however, it is demonstrated that no
                  simple definition is possible. A novel estimator of
                  the average generalization error (called \em{FPER})
                  is suggested and compared to the Final Prediction
                  Error (\em{FPE}) and Generalized Prediction Error
                  (\em{GPE}) estimators. In addition, comparative
                  numerical studies demonstrate the qualities of the
                  suggested estimator.},
}

