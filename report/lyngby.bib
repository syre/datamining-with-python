@InProceedings{VolbrachtSabine1997How,
  author = 	 {Sabine Volbracht and Gitta Domik and Khatoun
                  Shahrbabaki and Gregor Fels},
  title = 	 {How effective are 3D display modes?},
  booktitle = 	 {Conference on Human Factors in Computing Systems --- 
                  Proceedings}, 
  OPTcrossref =  {},
  OPTkey = 	 {},
  OPTeditor = 	 {},
  OPTvolume = 	 {},
  OPTnumber = 	 {},
  OPTseries = 	 {},
  year = 	 {1997},
  OPTorganization = {},
  publisher =    {ACM},
  OPTaddress = 	 {},
  month = 	 {March},
  pages = 	 {540--541},
  keywords =     {human computer interaction; human engineering; three
                  dimensional computer graphics; computer systems;
                  computer architecture; response time (computer
                  systems)}, 
  affiliation =  {Department. of Computer Science, University of
                  Paderborn},  
  URL =          {http://www.acm.org/sigchi/chi97/proceedings/tech-note/sbv.htm},
  note = 	 {Technical note},
  OPTannote = 	 {}, 
  abstract =     {The increasing availability of 3D input and output
                  devices demands a better understanding and
                  comparison of their quality. This report describes
                  an empirical experiment for comparing 3D display
                  modes: traditional perspective viewing, anaglyph
                  stereo and shutter glass stereo. We followed two
                  hypotheses 1. shutter glass stereo viewing allows a
                  faster and more accurate recognition than the
                  anaglyph and the perspective viewing, and
                  2. subjects experienced with particular 3D
                  representations are faster and more accurate than
                  subjects without experience. The experiment is based
                  on a true research scenario in organic
                  chemistry. Organic molecules were used as 3D
                  objects. Mean response error and mean response time
                  were calculated for a series of six tasks and 81
                  subjects.},
}

