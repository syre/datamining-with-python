@Article{Hsieh1994Urge,
  author = 	 {Jen-Chuen Hsieh and {\"O}sten H{\"a}germark and Mona
                  St{\aa}hle-B{\"a}ckdahl and Kaj Erikson and Lars
                  Erikson and Sharon Stone-Elander and Martin Ingvar},
  title = 	 {Urge to Scratch Represented in The Human Cerebral
                  Cortex During Itch},
  journal = 	 {Journal of Neurophysiology},
  year = 	 {1994},
  OPTkey = 	 {},
  volume = 	 {72},
  number = 	 {6},
  month = 	 {December},
  pages = 	 {3004--3008},
  obtain =       {copy in Hsieh's Ph.D. thesis (partly)},
  OPTnote = 	 {},
  OPTannote = 	 {},
  keywords =     {functional neuroimaging; pain},
  abstract =     {1. We used functional positron emission tomography
                  (PET), measuring regional cerebral blood flow (rCBF)
                  as an index of neuronal activity, to investigate the
                  central processing of itch in 10 healthy volunteers
                  subjected to intracutaneous injections of
                  histamine. 

                  2. The study has unraveled a central reprensentation
                  that depicts a motor intention of the urge to
                  scratch contigent on the perception of unpleasant
                  itch. The coactivation of the anterior congulate
                  cortex (ACC), supplementary motor area (SMA),
                  premotor area (PM), and inferior parietal lobule
                  (IPL) substantiates that the posterior sector of the
                  ACC (Brodmann 24) is related to the
                  sensorial/affectional aspect of the event. The
                  premotor cortical areas (SMA, PM) and the IPL may
                  participate in the preparation of an intended
                  action.},
}

