@Article{Aguirre1998Critique,
  author = 	 {Geoffrey Karl Aguirre and Eric Zarahn and
                  M. {D'Esposito}}, 
  title = 	 {A critique of the use of the {Kolmogorov-Smirnov}
                  ({KS}) statistic for the analysis of {BOLD} {fMRI}
                  data}, 
  journal = 	 {Magnetic Resonance in Medicine},
  year = 	 {1998},
  OPTkey = 	 {},
  volume = 	 {39},
  OPTnumber = 	 {},
  OPTmonth = 	 {},
  pages = 	 {500-505},
  OPTnote = 	 {},
  OPTannote = 	 {},
  keywords =     {functional neuroimaging; statistics; Kolmogorov-Smirnov},
  abstract =     {The use of the Kolmogorov-Smirnov (KS) statistic for
                  testing hypotheses regarding activation in BOLD fMRI
                  data is critiqued on both theoretical and empirical
                  grounds. Theoretically, it is argued that the KS
                  test is formally unable to support inferences of
                  interest to most neuro-imaging studies and has
                  reduced sensitivity compared to parametric
                  alternatives. Empirically, false positive rates
                  yielded by the KS test in human data collected under
                  the null hypothesis were significantly in excess of
                  tabular values. These excessive false-positive rates
                  could be explained by the presence of temporal
                  autocorrelation within null-hypothesis fMRI data. We
                  also present evidence that the distribution of BOLD
                  fMRI data is only slightly non-normal, questioning
                  the initial impetus for the use of the KS test in
                  this context. Finally, it is noted that parametric
                  alternatives exist which do provide adequate control
                  of the false-positive rate and can support
                  inferences of interest.},
}

