@Article{Hastie1995Penalized,
  author = 	 {Trevor Hastie and Andreas Buja and Robert J. Tibshirani},
  title = 	 {Penalized Discriminant Analysis},
  journal = 	 {The Annals of Statistics},
  year = 	 {1995},
  OPTkey = 	 {},
  volume = 	 {23},
  number = 	 {1},
  OPTmonth = 	 {},
  pages = 	 {73--102},
  OPTnote = 	 {},
  OPTannote = 	 {},
  keywords =     {Signal and image classification; discrimination;
                  regularization},
  abstract =     {Fisher's linear discriminant analysis (LDA) is a
                  popular data-analytic tool for studying the
                  relationship between a set of predictors and a
                  categorical response. In this paper we describe a
                  penalized version of LDA. It is designed for
                  situations in which there are many highly correlated
                  predictors, such as those obtained by discretizing a
                  functio, or the grey-scale values of the pixels in a
                  series of images. In cases such as these it is
                  natural, efficient and sometimes essential to impose
                  a spatial smopthness constraint on the
                  coefficients, both for improved prediction
                  performance and interpretability. We cast the
                  classification problem into a regression framework
                  via optimal scoring. Using this, our proposal
                  facilitates the use of any penalized regression
                  technique in the classification setting. The
                  technique is illustrated  with examples in speech
                  recognition and handwritten charater recognition.},
}

@Article{Hastie1996Discriminant,
  author = 	 {Trevor Hastie and Robert J. Tibshirani},
  title = 	 {Discriminant analysis by {Gaussian} mixtures},
  journal = 	 {Journal of the Royal Statistical Society, Series B,
                  Methodology},
  year = 	 {1996},
  OPTkey = 	 {},
  volume = 	 {58},
  number = 	 {1},
  OPTmonth = 	 {},
  pages = 	 {155--176},
  CiteSeer = {http://citeseer.ist.psu.edu/hastie96discriminant.html},
  OPTnote = 	 {},
  OPTannote = 	 {},
  keywords =     {classification, clustering, nonparametric
                  regression, pattern recognition, radial basis
                  functions},
  abstract =     {Fisher-Rao linear discriminant analysis (LDA) is a
                  valuable tool for multigroup classification. LDA is
                  equivalent to maximum likelihood classification
                  assuming Gaussian distributions for each class. In
                  this paper, we fit Gaussian mixtures to each class
                  to facilitate effective classification in non-normal
                  settings, especially when the classes are
                  clustered. Low dimensional views are an important
                  by-product of LDA - our new techniques inherit this
                  feature. We can control the within-class spread of
                  the subclass centres relative to the between-class
                  spread. Our technique for fitting these models
                  permits a natural blend with nonparametric versions
                  of LDA.},
}

