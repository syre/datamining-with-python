  pages = 	 {35--43},
  OPTnote = 	 {},
  OPTannote = 	 {},
  keywords =     {convergence of numerical methods; optimization},
  abstract =     {Makes use of a general theorem which gives
                  sufficient conditions for the convergence of a class
                  of search algorithms, to construct a convergent
                  conjugate directions algorithm for the unconstrained
                  minimization of real valued functions defined on
                  $R_n$. This algorithm is derived, by a simple
                  modification, from the Fletcher-Reeves
                  algorithm. Some numerical results illustrating the
                  behaviour of this new algorithm are given. It is
                  also shown that the convergence of a variable step
                  Newton method as well as that of the method of
                  steepest descent can be established by means of this
                  convergence theorem},
}

